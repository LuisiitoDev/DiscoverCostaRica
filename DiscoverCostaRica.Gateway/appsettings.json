{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "geoservice": {
        "ClusterId": "geoservice",
        "Match": {
          "Path": "/api/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api"
          }
        ]
      }
    },
    "Clusters": {
      "geoservice": {
        "Destinations": {
          "geoservice": {
            "Address": "https://geoservice"
          }
        }
      }
    }
  }
}
